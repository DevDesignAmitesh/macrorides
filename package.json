{
  "name": "main-code",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "migrate:db": "cd packages/db && npx prisma migrate dev -n 'init'",
    "generate:db": "cd packages/db && npx prisma generate",
    "build:db": "cd packages/db && pnpm build",
    "build:types": "cd packages/types && pnpm build",
    "build:packages": "pnpm --filter ./packages/** run build",
    "final:build:packages": "pnpm i && pnpm run build:db && pnpm run build:types && pnpm run build:packages",
    "build:be": "pnpm i && cd apps/http-backend && pnpm build",
    "build:vendor-app": "pnpm i && cd apps/vendor-app && pnpm build",
    "start:vendor-app": "cd apps/vendor-app && pnpm start",
    "start:be": "cd apps/http-backend && pnpm start"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.6.3",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.26.0"
}
