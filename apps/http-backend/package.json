{
  "name": "http-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -b",
    "build:node": "tsc -b",
    "dev": "pnpm run build && pnpm run start",
    "start": "node dist/bin.js",
    "build:packages": "cd ../../ && pnpm --filter ./packages/** build",
    "build:types": "pnpm --filter @repo/types build",
    "build:maps": "pnpm --filter @repo/maps build",
    "build:email": "pnpm --filter @repo/email build",
    "build:ui": "pnpm --filter @repo/ui build",
    "build:otp": "pnpm --filter @repo/otp build",
    "build:db": "pnpm --filter @repo/db build",
    "generate:db": "cd ../../ && cd packages/db && npx prisma generate",
    "build:http-backend": "pnpm run generate:db && pnpm run build:types && pnpm run build:ui && pnpm run build:email && pnpm run build:maps && pnpm run build:db && pnpm run build:otp && pnpm run build:packages && pnpm turbo run build:node --filter=http-backend"
  },
  "devDependencies": {
    "@repo/db": "workspace:*",
    "@repo/email": "workspace:*",
    "@repo/maps": "workspace:*",
    "@repo/otp": "workspace:*",
    "@repo/types": "workspace:*",
    "@repo/typescript-config": "workspace:*"
  },
  "keywords": [],
